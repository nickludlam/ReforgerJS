{
  "server": {
    "id": 1,
    "name": "SERVER NAME",
    "host": "xxx.xxx.xxx.xxx",
    "queryPort": 1111,
    "rconPort": 1111,
    "rconPassword": "PASSWORD",
    "logReaderMode": "tail",
    "filename": "console.log",
    "backfill": true,
    "logDir": "CC:/path/to/reforger/log/folder",
    "sftp": {
      "host": "xxx.xxx.xxx.xxx",
      "port": 21,
      "username": "SFTP Username",
      "password": "SFTP Password"
    },
    "ftp": {
      "host": "xxx.xxx.xxx.xxx",
      "port": 21,
      "user": "FTP Username",
      "password": "FTP Password"
    }
  },
  "consoleLogLevel": "info",
  "outputLogLevel": "info",
  "connectors": {
    "discord": {
      "token": "",
      "clientId": "",
      "guildId": ""
    },
    "mysql": {
      "enabled": false,
      "host": "mysql-host",
      "port": 3306,
      "username": "reforgerjs",
      "password": "password",
      "database": "reforgerjs",
      "dialect": "mysql"
    }
  },
  "roles": {
    "roleName": "discord RoleID",
    "roleName1": "discord RoleID",
    "roleName2": "discord RoleID",
    "roleName3": "discord RoleID"
  },
  "roleLevels": {
    "1": [
      "roleName",
      "roleName1"
    ],
    "2": [
      "roleName2"
    ],
    "3": [
      "roleName3"
    ]
  },
  "commands": [
    {
      "command": "whois",
      "enabled": false,
      "commandLevel": 3
    },
    {
      "command": "stats",
      "enabled": false,
      "commandLevel": 3,
      "statsTable": ""
    },
    {
      "command": "rcon",
      "enabled": true,
      "commandLevel": 3,
      "ban": 3,
      "shutdown": 3,
      "restart": 3,
      "kick": 3
    }
  ],
  "plugins": [
    {
      "plugin": "LogVoteKickVictim",
      "enabled": false,
      "channel": ""
    },
    {
      "plugin": "LogVoteKickStart",
      "enabled": false,
      "channel": ""
    },
    {
      "plugin": "DBLog",
      "enabled": false,
      "interval": 5
    },
    {
      "plugin": "DBLogStats",
      "enabled": false,
      "interval": 5,
      "path": "C:/path/to/saves/profile/.save/playersave",
      "tableName": ""
    },
    {
      "plugin": "SeedTrackerBasic",
      "enabled": false,
      "interval": 5,
      "seedStart": 4,
      "seedEnd": 40
    },
    {
      "plugin": "AltChecker",
      "enabled": false,
      "channel": "",
      "logAlts": true,
      "logOnlyOnline": false
    },
    {
      "plugin": "ServerStatus",
      "enabled": false,
      "channel": "",
      "messageID": "",
      "interval": 1,
      "showFPS": true,
      "showMemoryUsage": false,
      "discordBotStatus": true,
      "embed": {
          "title": "Arma Reforger Server Status",
          "color": "#00FF00",
          "footer": "ReforgerJS",
          "thumbnail": false,
          "thumbnailURL": "https://IMAGE_URL_HERE.png"
      }
    }
  ]
}